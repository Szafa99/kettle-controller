{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc.exe kompiluj aktywny plik",
			"command": "C:\\MinGW\\bin\\gcc.exe",
			"args": [
				"-g",
				"-pthread",
				"${file}",
				"-o",
				"${fileDirname}\\${fileBasenameNoExtension}.exe"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "kompilator: C:\\MinGW\\bin\\gcc.exe"
		},
		{
			"type": "cppbuild",
			"label": "C/C++: gcc.exe kompiluj aktywny plik",
			"command": "C:\\MinGW\\bin\\gcc.exe",
			"args": [
				"-g",
				"-pthread",
				"${file}",
				"-o",
				"${fileDirname}\\${fileBasenameNoExtension}.exe"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "kompilator: C:\\MinGW\\bin\\gcc.exe"
		},
		{
			"type": "cppbuild",
			"label": "C/C++: cl.exe kompiluj aktywny plik",
			"command": "cl.exe",
			"args": [
				"/Zi",
				"/EHsc",
				"/nologo",
				"/Fe:",
				"${fileDirname}\\${fileBasenameNoExtension}.exe",
				"${file}"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$msCompile"
			],
			"group": "build",
			"detail": "kompilator: cl.exe"
		}
	]
}